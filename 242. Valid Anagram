bool isAnagram(char * s, char * t){
    int sLen=strlen(s);
    int tLen=strlen(t);
    if(sLen!=tLen) return false;
    else
    {
        int sSet[26]={0};
        int tSet[26]={0};
        for(int i=0;i<sLen;i++)
        {
            sSet[s[i]-'a']++;
            tSet[t[i]-'a']++;
        }
        for(int i=0;i<26;i++)
        {
            if(sSet[i]!=tSet[i]) return false;
        }
        return true;
    }
}
